<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">

  <div id="dt-proxyConfigContainer">
    <f:entry title="${%type}" field="type">
      <f:enum>
        ${it.name()}
      </f:enum>
    </f:entry>
    <f:entry title="${%host}" field="host">
      <f:textbox name="host"/>
    </f:entry>
    <f:entry title="${%port}" field="port">
      <f:number name="port"/>
    </f:entry>
  </div>

</j:jelly>
